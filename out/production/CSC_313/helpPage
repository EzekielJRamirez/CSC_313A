        }
        for (int i = 0; i < input.size(); i++) {
            //insert A[i] into list B[n * A[i]] with floor round
            // dividing by 100 makes numbers any 2 digit number less than 1     78 --> .78
//            Integer temp = n * input.elementAt(i) / 100;
